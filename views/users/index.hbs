
<h1>All Users</h1>

<div class="right" id='index'>
<ul>
  {{#each users}}
  <li>
    <a href="/users/{{_id}}">
     
      <img src={{this.pic}} class ='circle responsive-img' alt={{this.name}} width='200'>
      <p>{{name}}</p>
      <p>{{email}}</p>
      
    </a>
  </li>
  {{/each}}
</ul>

</div>
